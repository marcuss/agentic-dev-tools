# Jira/Confluence API Configuration
# Copy this file to ~/.junie/.env/jira_confluence.env and fill in your values
# Then set proper permissions: chmod 600 ~/.junie/.env/jira_confluence.env

# Atlassian API Key (create at https://id.atlassian.com/manage-profile/security/api-tokens)
JIRA_API_KEY="your-api-key"

# Your Atlassian domain (without https:// prefix)
JIRA_DOMAIN="your-domain.atlassian.net"
CONFLUENCE_DOMAIN="your-domain.atlassian.net"

# Email address associated with your Atlassian account
JIRA_USER_EMAIL="your-email@example.com"

# GitHub Repository Configuration
# Settings for accessing and managing the repository

# GitHub Personal Access Token (create at https://github.com/settings/tokens)
AGENTIC_DEV_TOOLS_REPO_TOKEN="your-github-token"

# GitHub username
AGENTIC_DEV_TOOLS_REPO_USER="your-github-username"

# Email associated with your GitHub account
AGENTIC_DEV_TOOLS_REPO_USER_EMAIL="your-github-email@example.com"

# Amplitude/Statsig API Configuration
# API keys for accessing Amplitude and Statsig services

# Amplitude Management API Key (create at https://experiment.amplitude.com/settings/management-api)
AMPLITUDE_MANAGEMENT_API_KEY="your-amplitude-api-key"

# Statsig Console API Key (create at https://console.statsig.com/settings/keys)
STATSIG_CONSOLE_API_KEY="your-statsig-api-key"